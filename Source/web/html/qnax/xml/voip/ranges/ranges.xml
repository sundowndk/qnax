<sui>
	<container width="100%" height="100%" title="Ranges" icon="Icon32Edit">		
		<layoutbox type="vertical" width="100%" height="100%">
			<panel size="*">
				<listview tag="ranges" width="100%" height="100%" treeview="true" treeviewLinkColumns="id:countrycodeid">
					<column tag="id" />						
					<column tag="name" label="Name" width="300px" visible="true" />
					<column tag="countrycodeid" width="100px" />
					<column tag="dialcodes" label="Dialcodes" condense="value" width="100px" visible="true" />
				</listview>
			</panel>
			<panel size="100px">
				<button tag="rangescreate" stylesheet="SUIButtonSmall" width="100%" label="Create" />
				<button tag="rangesedit" stylesheet="SUIButtonSmall" width="100%" label="Edit" />
				<button tag="rangesdelete" stylesheet="SUIButtonSmall" width="100%" label="Delete" />				
			</panel>
		</layoutbox>				
	</container>	
</sui>
